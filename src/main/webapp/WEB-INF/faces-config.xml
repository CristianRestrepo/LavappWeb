<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>Dashboard</from-outcome>
            <to-view-id>/Vistas/Usuario/Dashboard/Dashboard.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>Principal</from-outcome>
            <to-view-id>/Vistas/Cliente/Pedidos/Pedidos.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>Pais</from-outcome>
            <to-view-id>/Vistas/Usuario/Ubicaciones/Pais.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>Departamento</from-outcome>
            <to-view-id>/Vistas/Usuario/Ubicaciones/Departamento.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>Ciudad</from-outcome>
            <to-view-id>/Vistas/Usuario/Ubicaciones/Ciudad.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>Localidad</from-outcome>
            <to-view-id>/Vistas/Usuario/Ubicaciones/Localidad.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>Barrio</from-outcome>
            <to-view-id>/Vistas/Usuario/Ubicaciones/Barrio.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>Zona</from-outcome>
            <to-view-id>/Vistas/Usuario/Ubicaciones/Zona.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>Estrato</from-outcome>
            <to-view-id>/Vistas/Usuario/Ubicaciones/Estrato.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    
</faces-config>
